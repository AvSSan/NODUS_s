# PostgreSQL
POSTGRES_DB=nodus
POSTGRES_USER=nodus
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD_HERE

# JWT (Минимум 32 символа!)
JWT_SECRET_KEY=CHANGE_ME_VERY_LONG_RANDOM_SECRET_KEY_AT_LEAST_32_CHARS
JWT_REFRESH_SECRET_KEY=CHANGE_ME_ANOTHER_VERY_LONG_SECRET_KEY_FOR_REFRESH

# Token expiration
ACCESS_TOKEN_EXPIRES_MINUTES=15
REFRESH_TOKEN_EXPIRES_MINUTES=10080

# MinIO S3
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=CHANGE_ME_MINIO_PASSWORD_HERE
